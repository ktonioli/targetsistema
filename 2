def fibonacci_sequence(n):
    # Inicializando a sequência de Fibonacci
    fib_sequence = [0, 1]
    
    # Gerando a sequência até o valor ser maior ou igual a n
    while fib_sequence[-1] < n:
        next_value = fib_sequence[-1] + fib_sequence[-2]
        fib_sequence.append(next_value)
    
    return fib_sequence

def is_in_fibonacci(n):
    # Gera a sequência de Fibonacci até o valor n
    fib_sequence = fibonacci_sequence(n)
    
    # Verifica se o número informado está na sequência
    if n in fib_sequence:
        return f"O número {n} pertence à sequência de Fibonacci."
    else:
        return f"O número {n} não pertence à sequência de Fibonacci."

# Exemplo de uso:
numero = int(input("Informe um número: "))
resultado = is_in_fibonacci(numero)
print(resultado)
